name: ax-hpo-mnist

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      max_epochs: {type: int, default: 3}
      total_trials: {type: int, default: 3}


    command: |
          python ax_hpo_mnist.py \
            --max_epochs {max_epochs} \
            --total_trials {total_trials}

